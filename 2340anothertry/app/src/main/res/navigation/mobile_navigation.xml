<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_home">

    <fragment
        android:id="@+id/nav_home"
        android:name="com.example.a2340anothertry.ui.home.HomeFragment"
        android:label="@string/menu_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_nav_home_to_nav_d"
            app:destination="@id/nav_d" />
    </fragment>
    <fragment
        android:id="@+id/nav_d"
        android:name="com.example.a2340anothertry.ui.d.dFragment"
        android:label="Create Task"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_nav_d_to_nav_home"
            app:destination="@id/nav_home" />
    </fragment>
    <fragment
        android:id="@+id/nav_c"
        android:name="com.example.a2340anothertry.ui.c.cFragment"
        android:label="Create Assignment"
        tools:layout="@layout/fragment_c" >
        <action
            android:id="@+id/action_nav_c_to_nav_gallery"
            app:destination="@id/nav_gallery" />
    </fragment>
    <fragment
        android:id="@+id/nav_a"
        android:name="com.example.a2340anothertry.ui.a.aFragment"
        android:label="Create Exam"
        tools:layout="@layout/fragment_a" >
        <action
            android:id="@+id/action_nav_a_to_nav_slideshow"
            app:destination="@id/nav_slideshow" />
    </fragment>
    <fragment
        android:id="@+id/nav_b"
        android:name="com.example.a2340anothertry.ui.b.bFragment"
        android:label="Create Class"
        tools:layout="@layout/fragment_b" >
        <action
            android:id="@+id/action_nav_b_to_nav_classes"
            app:destination="@id/nav_classes" />
    </fragment>

    <fragment
        android:id="@+id/nav_gallery"
        android:name="com.example.a2340anothertry.ui.gallery.GalleryFragment"
        android:label="@string/menu_gallery"
        tools:layout="@layout/fragment_gallery" >
        <action
            android:id="@+id/action_nav_gallery_to_nav_c"
            app:destination="@id/nav_c" />
        <action
            android:id="@+id/action_nav_gallery_to_nav_c_edit"
            app:destination="@id/nav_c_edit" />
    </fragment>

    <fragment
        android:id="@+id/nav_c_edit"
        android:name="com.example.a2340anothertry.ui.c.cEditFragment"
        android:label="Edit Assignment"
        tools:layout="@layout/fragment_c_edit" >
        <action
            android:id="@+id/action_nav_c_edit_to_nav_gallery"
            app:destination="@id/nav_gallery" />
    </fragment>

    <fragment
        android:id="@+id/nav_slideshow"
        android:name="com.example.a2340anothertry.ui.slideshow.SlideshowFragment"
        android:label="@string/menu_slideshow"
        tools:layout="@layout/fragment_slideshow" >
        <action
            android:id="@+id/action_nav_slideshow_to_nav_a"
            app:destination="@id/nav_a" />
    </fragment>
    <fragment
        android:id="@+id/nav_classes"
        android:name="com.example.a2340anothertry.ui.classes.ClassesFragment"
        android:label="@string/menu_classes"
        tools:layout="@layout/fragment_classes" >
        <action
            android:id="@+id/action_nav_classes_to_nav_b"
            app:destination="@id/nav_b" />
    </fragment>
</navigation>